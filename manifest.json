{
  "manifest_version": 3,
  "name": "IRCTC AutoFill",
  "version": "1.0",
  "description": "Auto-fills passenger details on IRCTC",
  "permissions": ["scripting"],
  "host_permissions": ["https://www.irctc.co.in/*"],
  "content_scripts": [
    {
      "matches": ["https://www.irctc.co.in/nget/booking/psgninput"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
